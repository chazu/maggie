# Success.mag

Success subclass: Result

  # Testing
  method: isSuccess [
      ^true
  ]

  method: isFailure [
      ^false
  ]

  # Accessing
  method: value [
      ^self primValue
  ]

  method: error [
      ^nil
  ]

  # Printing
  method: printString [
      ^'Success(', self value printString, ')'
  ]
