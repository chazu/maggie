# Failure.mag

Failure subclass: Result

  # Testing
  method: isSuccess [
      ^false
  ]

  method: isFailure [
      ^true
  ]

  # Accessing
  method: value [
      ^nil
  ]

  method: error [
      ^self primError
  ]

  # Printing
  method: printString [
      ^'Failure(', self error printString, ')'
  ]
