# Symbol.mag

Symbol subclass: Object

  # Symbols are unique and immutable
  method: asSymbol [
      ^self
  ]

  method: asString [
      ^self primAsString
  ]

  # Comparison (identity for symbols)
  method: = other [
      ^self == other
  ]

  # Printing
  method: printString [
      ^'#', self asString
  ]
