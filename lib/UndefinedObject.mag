# UndefinedObject.mag

UndefinedObject subclass: Object

  # Nil testing
  method: isNil [
      ^true
  ]

  method: notNil [
      ^false
  ]

  # Conditional evaluation
  method: ifNil: block [
      ^block value
  ]

  method: ifNotNil: block [
      ^nil
  ]

  method: ifNil: nilBlock ifNotNil: notNilBlock [
      ^nilBlock value
  ]

  method: ifNotNil: notNilBlock ifNil: nilBlock [
      ^nilBlock value
  ]

  # Printing
  method: printString [
      ^'nil'
  ]
